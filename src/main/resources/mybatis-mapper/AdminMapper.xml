<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kelvin.joyinternetcafe.mapper.IAdminMapper">
    <select id="selectAdminPwd" resultType="String" parameterType="String">
        select password from admin where `name` = #{name};
    </select>
</mapper>
